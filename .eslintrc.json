{
  "extends": [
    "prettier"
  ],
  "parser": "babel-eslint",
  "plugins": ["prettier"],
  "rules": {
    "import/prefer-default-export": 0
  },
  "globals": {
    "window": true,
    "document": true,
    "localStorage": true,
    "navigator": true,
    "FormData": true,
    "FileReader": true,
    "Blob": true
  },
  "settings": {
    "import/resolver": {
      "node": {
        "moduleDirectory": ["node_modules", "src/"]
      }
    }
  },
  "env": {
    "jest": true,
    "browser": true
  }
}
